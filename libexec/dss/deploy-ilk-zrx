#!/usr/bin/env bash

# shellcheck source=bin/lib/common.sh
. "$LIB_DIR/common.sh"

if [[ -z "$ZRX" ]]; then
    ZRX=$(dappCreate dss-deploy ZRX "$(seth --to-uint256 "$(seth --to-wei 1000000 ETH)")")
    log "ZRX=$ZRX"
    export ZRX
fi

# shellcheck source=/dev/null
source "$LIBEXEC_DIR/dss/deploy-ilk" "ZRX" "${1:-"A"}" "GemJoin"
