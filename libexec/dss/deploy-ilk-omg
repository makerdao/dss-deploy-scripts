#!/usr/bin/env bash

# shellcheck source=bin/lib/common.sh
. "$LIB_DIR/common.sh"

if [[ -z "$OMG" ]]; then
    OMG=$(dappCreate dss-deploy OMG "$(seth --to-uint256 "$(seth --to-wei 1000000 ETH)")")
    log "OMG=$OMG"
    export OMG
fi

# shellcheck source=/dev/null
source "$LIBEXEC_DIR/dss/deploy-ilk" "OMG" "${1:-"A"}" "GemJoin2"
